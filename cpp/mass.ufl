cell_type = tetrahedron
degree = 5

element = FiniteElement("Lagrange", cell_type, degree)
coord_element = VectorElement("Lagrange", cell_type, 1)
mesh = Mesh(coord_element)

V = FunctionSpace(mesh, element)

u = TrialFunction(V)
v = TestFunction(V)
f = Coefficient(V)
kappa = Constant(mesh)
a = kappa * inner(grad(u), grad(v)) * dx